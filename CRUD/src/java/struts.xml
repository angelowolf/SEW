<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <constant name="struts.devMode" value="true"/>
    <package name="default" extends="struts-default" namespace="/Candidato">
        <action name="guardarOModificarCandidato" method="guardarOModificarCandidato" class="Action.CandidatoAction">
            <result name="success" type="redirect">listCandidato</result>
        </action>
        <action name="eliminarCandidato" method="eliminar" class="Action.CandidatoAction">
            <result name="success" type="redirect">listCandidato</result>
        </action>
        <action name="listCandidato" method="list" class="Action.CandidatoAction">
            <result name="success">/Candidato/Todos.jsp</result>
        </action>
        <action name="editarCandidato" method="editar" class="Action.CandidatoAction">
            <result name="success">/Candidato/AltaModificacion.jsp</result>
        </action>       
    </package>
</struts>